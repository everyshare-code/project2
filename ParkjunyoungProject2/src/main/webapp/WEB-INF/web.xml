<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  
    <jsp-config>
    <taglib>
      <taglib-uri>/WEB-INF/tlds/boardtaglib.tld</taglib-uri>
      <taglib-location>/WEB-INF/tlds/boardtaglib.tld</taglib-location>
    </taglib>
  </jsp-config>
  
  <context-param>
  	<param-name>JNDI-ROOT</param-name>
  	<param-value>java:comp/env/jdbc/ictuser</param-value>
  </context-param>
  <context-param>
  	<param-name>ACCESS</param-name>
  	<param-value>accessToken</param-value>
  </context-param>
  <context-param>
  	<param-name>REFRESH</param-name>
  	<param-value>refreshToken</param-value>
  </context-param>
  
  
  
  
  <welcome-file-list>
    <welcome-file>index.do</welcome-file>
  </welcome-file-list>
</web-app>